#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules/cssesc/bin/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules/cssesc/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/node_modules:/mnt/d/ESTUDIO/Programacion/node_modules:/mnt/d/ESTUDIO/node_modules:/mnt/d/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules/cssesc/bin/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules/cssesc/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/node_modules:/mnt/d/ESTUDIO/Programacion/node_modules:/mnt/d/ESTUDIO/node_modules:/mnt/d/node_modules:/mnt/d/ESTUDIO/Programacion/Astro/Porfolio/festupinans/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../cssesc/bin/cssesc" "$@"
fi
